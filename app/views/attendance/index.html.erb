<div class="container">


<div class="row">
<%@all_attendances.each do |elem|%>
  <div class="col-md-6">
    <div class="card mb-2">
      <div class="card-body">
        <h5 class="card-title"><%= elem.stripe_customer_id %></h5>
        <h5 class="card-title"><%= elem.user.first_name%> <%= elem.user.last_name%></h5>
      </div>
    </div>
  </div>
<% end %>
</div>
</div>